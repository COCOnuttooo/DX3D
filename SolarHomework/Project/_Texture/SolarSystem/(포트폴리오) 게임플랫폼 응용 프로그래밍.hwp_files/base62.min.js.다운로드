function Base62(){this.baseDigits="0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"}Base62.prototype.decode=function(a){var b=typeof a;if(b!=="string"){throw new TypeError("parameter should be a string: "+b)}if(!/^-?[\da-zA-Z]+$/.test(a)){throw new Error("parameter is unsupported format: "+a)}for(decodedNum=0,i=len=a.length;i--;){decodedNum+=this.baseDigits.indexOf(a[i])*Math.pow(62,len-i-1)}return decodedNum};Base62.prototype.encode=function(a){if(isNaN(parseInt(a))||a===0){return"0"}else{a=parseInt(a)}encodedStr="";while(a>0){encodedStr=this.baseDigits.charAt(a%62)+encodedStr;a=Math.floor(a/62)}return encodedStr};var obj=new Base62();(function(a){a.encodeBase62=function c(d){return obj.encode(d)};a.decodeBase62=function b(d){return obj.decode(d)}})(jQuery);