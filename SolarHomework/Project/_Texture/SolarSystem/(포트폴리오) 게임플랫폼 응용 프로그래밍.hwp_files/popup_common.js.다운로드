var PopupManagement = new function() {
	var popupBox = null;
	var stack = new Array();

	this.getCurrentPopup = function() {
		return popupBox;
	};

	this.pushPopup = function(popupObj) {
		popupBox = popupObj;
		stack.push(popupBox);
	};

	this.removePopup = function() {
		if (stack.length == 0) {
			popupBox = null;
			return popupBox;
		}

		var popupObj = stack.pop();
		if (stack.length == 0)
			popupBox = null;
		else
			popupBox = stack[0];

		return popupObj;
	};
}();

