var PopupContentUI = new function() {
	this.getDocumentIcon = function(type) {
		return '<img src="' + CF_PATH + '/maxage1/common/img/v4/s_' + type + '.svg" />';
	};

	this.getNoticeCapacityMessage = function(size) {
		return LanguagePack.POPUP_FILECAPACITY + size + LanguagePack.POPUP_EXCESS_AMOUNT;
	};

	this.getBubbleMessageUI = function(className, message) {
		var ui = '<p class="txt ' + className + ' show"><img alt="" src="' + CF_PATH + '/resources/image/Polaris/arrow_disc.png" class="arrow" />' + message + '</p>';
		return ui;
	};

	this.getBubbleMessageUI2 = function(message, top, left) {
		var ui = '<p class="txt txt_1 show" style="top:'+ top + 'px;'+ ((left != undefined && left != null) ? 'left:'+ left + 'px;' : '') +'"><img alt="" src="' + CF_PATH + '/resources/image/Polaris/arrow_disc.png" class="arrow" />' + message + '</p>';
		return ui;
	};

	this.DEFAULT =
			'<p id="popup_message_default_message" class="txt_3line"></p>';

	this.getNoticeUI = function(subtitle, messages) {
		function appendNoticeMessages() {
			var length = messages.length;
			for (var i = 0; i < length; i++)
				ui += '	<li><span>' + messages[i] + '</span></li>';
			'</ul>';
		}

		var ui =
			'<div class="txt_set01">' + subtitle + '</div>' +
			'<ul class="notice">';

		appendNoticeMessages();
		return ui;
	};

	this.URL_COPY =
		'<div class="bx_url">' +
//		'	<div class="photo_portrait"><img id="popup_url_copy_thumbnail" src="" /></div>' +
//		'	<div id="popup_url_copy_filename" class="url"></div>' +
//		'	<div class="input"><input id="popup_url_copy_weblink" type="text" value="" readonly /></div>' +
		'	<div class="photo_portrait"><img src="' + CF_PATH + '/maxage1/common/img/v4/s_pptx.svg" /></div>' +
		'	<div id="popup_url_copy_filename" class="url"></div>' +
		'	<dl class="input">' +
		'		<dt class="input_dt"><input type="text" id="popup_url_copy_weblink" readonly value="" /></dt>' +
