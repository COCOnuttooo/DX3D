			'</ul>';
	this.INPUTBOX = function(type, label, value) {
		var str = '<p id="popup_message_default_message" class="txt_3line"></p>' + '<div class="input_setting">';
		if(typeof value != "undefined" && value !="") {
			str += ' <label class="pos off" for="popup_account_deletion_password">' + label + '</label>';
		} else {
			value = "";
			str += ' <label class="pos" for="popup_account_deletion_password">' + label + '</label>';
		}
		str += ' <input id="popup_account_deletion_password" class="setting" type="' + type + '" value="' + value + '"/>' + '</div>';
		return str;
	};

	this.PASSWORD_REGIST =
		'<div class="txt_set02">' + LanguagePack.ENTERSERVICEPASSWORD +
		'</div>' +
		'<div class="input_setting">' +
		' <label class="pos" for="popup_password_regist">' + LanguagePack.KEYWORD_PASSWORD + '</label>' +
		'	<input id="popup_password_regist" class="setting" type="password" />' +
		'</div>';
	
	this.setPasswordStr = function(_msg, _pwdtoggle){
		var addtoggledel = ' <label class="pos" for="popup_account_deletion_password">' + LanguagePack.KEYWORD_PASSWORD + '</label>';
		var addtogglereg = ' <label class="pos" for="popup_password_regist">' + LanguagePack.KEYWORD_PASSWORD + '</label>';
		var addtoggle = '';
		var placeholder = '';
		if(_pwdtoggle) {
			placeholder = ' placeholder="' + LanguagePack.KEYWORD_PASSWORD +'" ';
			addtoggledel = ' <input id="popup_account_deletion_password2" class="setting" type="text" '+placeholder+' />';
			addtogglereg = ' <input id="popup_password_regist2" class="setting" type="text" '+placeholder+' />';
			addtoggle = '<a href="#" class="hidepwd"><img src="' + CF_PATH + '/resources/image/Polaris/new/popup/notice/pw_hide.png"></a>'+
			'<a href="#" class="showpwd"><img src="' + CF_PATH + '/resources/image/Polaris/new/popup/notice/pw_show.png"></a>';
		}
		this.ACCOUNT_DELETION =
			'<div class="txt_set02">' + _msg +
			'</div>' +
			'<div class="input_setting">' +  addtoggledel +
			'	<input id="popup_account_deletion_password" class="setting" type="password" '+placeholder+' />' + addtoggle +
			'</div>';

		this.SET_EMAIL =
			'<div class="txt_set02">' + _msg +
			'</div>' +
			'<div class="input_setting">' +
			' <label class="pos" for="popup_account_deletion_password">' + LanguagePack.BUBBLE_ENTERYOUREMAIL + '</label>' +
			'	<input id="popup_account_deletion_password" class="setting" type="text" />' +
			'</div>';

		this.PASSWORD_REGIST =
			'<div class="txt_set02">' + _msg +
			'</div>' +
			'<div class="input_setting">' +  addtogglereg +
			'	<input id="popup_password_regist" class="setting" type="password" '+placeholder+' />' + addtoggle +
			'</div>';
		
		this.ACCOUNT_MERGE =
			'<div class="txt_set02">' + _msg +
			'</div>' +
			'<div class="input_setting">' +  addtoggledel +
			'	<input id="popup_account_deletion_password" class="setting" type="password" '+placeholder+' />' + addtoggle +
			'</div>';
	};
	this.setEmailAndPwd = function(_msg, _content){
		var con_str = LanguagePack.CHINAVER_SET_EMAIL_PWD_STR;
		var msg_str = LanguagePack.BUBBLE_ENTERYOUREMAIL;
		if(_msg) {
			msg_str = _msg;
		}
		if(_content) {
			con_str = _content;
		}
		this.ACCOUNT_CHANGE =
			'<div class="txt_set02" style="padding-bottom: 0px;">' + con_str + '</div>' +
			'<ul class="input_setting2" style="padding: 20px 0;">' +
			'	<li class="first"><input id="popup_account_change_newaccount" class="setting" type="text" value="" placeholder="' + msg_str + '" /></li>' +
			'	<li><input id="popup_account_change_password2" class="setting" type="text" placeholder="' + LanguagePack.KEYWORD_PASSWORD +'" />'+
			'<input id="popup_account_change_password" class="setting" type="password" placeholder="' + LanguagePack.KEYWORD_PASSWORD +'" />'+
			'<a href="#" class="hidepwd"><img src="' + CF_PATH + '/resources/image/Polaris/new/popup/notice/pw_hide.png"></a>'+
			'<a href="#" class="showpwd"><img src="' + CF_PATH + '/resources/image/Polaris/new/popup/notice/pw_show.png"></a>'+
			'</li></ul>';
	};
	this.PROMOTION =
		'<p id="popup_message_default_message" class="promotion_txt txt_3line"></p>';

}();

